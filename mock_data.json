        {
            "acf_fc_layout": "layout_content_block",
            "content_item": "<h2>ILLUSTRATIONS</h2>\n<p>The idea was to develop illustrations that is in trend with the storyline of youths in their respective stages in lives, performing various activities.</p>\n",
            "background_color": "",
            "text_color": "#000000"
        },
